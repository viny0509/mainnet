{
    "name": "script",
    "version": "1.0.0",
    "description": "Script",
    "main": "server.js",
    "license": "MIT",
    "scripts": {
        "start": "npm run serve",
        "build": "tsc && tsc-alias",
        "build:watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
        "serve": "node dist/server.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run build:watch\" \"npm run watch-node\"",
        "watch-node": "nodemon dist/server.js",
        "lint": "eslint src/*.ts",
        "lint-fix": "yarn lint --fix",
        "debug": "npm run build && npm run watch-debug",
        "serve-debug": "nodemon --inspect dist/server.js",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run build:watch\" \"npm run serve-debug\"",
        "typecheck": "tsc --noEmit",
        "run-prettier": "prettier src/*.ts",
        "validate-prettiness": "yarn run-prettier -c",
        "make-prettier": "yarn run-prettier --write"
    },
    "author": "nhatminh0509",
    "dependencies": {
        "@1inch/permit-signed-approvals-utils": "^1.4.8",
        "@types/qs": "^6.9.7",
        "alchemy-sdk": "^2.8.2",
        "axios": "^1.3.5",
        "cors": "^2.8.5",
        "cron": "^2.2.0",
        "dotenv": "^16.0.3",
        "ethers": "5.5.1",
        "joi": "^17.9.1",
        "lodash": "^4.17.21",
        "mongoose": "^6.9.0",
        "qs": "^6.11.1",
        "query-string": "^8.1.0",
        "web3": "^1.9.0"
    },
    "devDependencies": {
        "@sentio/ethers-v6": "^1.0.25",
        "@types/cors": "^2.8.13",
        "@types/cron": "^2.0.0",
        "@types/lodash": "^4.14.194",
        "@types/node": "14.18.36",
        "@typescript-eslint/eslint-plugin": "4.33.0",
        "@typescript-eslint/parser": "4.33.0",
        "concurrently": "6.5.1",
        "eslint": "7.32.0",
        "eslint-config-prettier": "8.6.0",
        "eslint-plugin-import": "2.27.5",
        "nodemon": "2.0.20",
        "prettier": "2.8.4",
        "tsc-alias": "^1.8.5",
        "typescript": "4.9.5"
    }
}
